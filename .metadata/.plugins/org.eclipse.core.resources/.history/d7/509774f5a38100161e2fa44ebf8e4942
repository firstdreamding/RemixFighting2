package entities;

import java.awt.Rectangle;

public class Hurtbox extends Rectangle {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	public int knockback;
	private Entity e;

	public Hurtbox(Entity e, int w, int h, int x, int y) {
		this.width = w;
		this.height = h;
		
		this.x = x;
		this.y = y;
		this.e = e;
	}

	public Entity getEntity() {
		return e;
	}

}
